<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title or renderData.title or metadata.title }}</title>
  <script async src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
  {% set css %}
    {% include "assets/css/styles.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
      {% if permalink.includes('projects') %}
    <script src="/_includes/assets/js/simpleLightbox.min.js"></script>
    {% endif %}
          {% if permalink.includes('contact') %}

    <script src="/_includes/assets/js/form.js" defer></script>
    {% endif %}
    <script src="/_includes/assets/js/bundle.js"></script>
</head>


